extends layout

include mixins/_songView

block content
  .row.justify-content-md-center
    .col-md-8.col-sm-12
      +songView(song)
      .chord__list.mt-5
        h5 Chords
        table.table
          tbody
            tr
              each chord in chords
                td.chord__key
                  a(href=`/public/uploads/${chord.file}`) #{chord.key}
                  
